node('u4')
{
    stage('git'){
        git 'https://github.com/rajeshande430/springpetclinic.git'
    }
    stage('compile'){
        sh 'mvn compile'
    }
    stage('test'){
        sh 'mvn test'
    }
    stage('package'){
        sh 'mvn package'
    }
    stage('artifacts'){
        archive 'target/*.jar'
    }
}